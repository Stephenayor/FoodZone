<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_admin_side"
    app:startDestination="@id/adminHomeFragment">


    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.example.foodzone.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
    </fragment>
    <fragment
        android:id="@+id/adminHomeFragment"
        android:name="com.example.foodzone.adminside.AdminHomeFragment"
        android:label="fragment_admin_home"
        tools:layout="@layout/fragment_admin_home" >
    <action
        android:id="@+id/action_adminHomeFragment_to_userActivity"
        app:destination="@id/userActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.foodzone.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/userActivity"
        android:name="com.example.foodzone.UserActivity"
        android:label="activity_user"
        tools:layout="@layout/activity_user" />
</navigation>